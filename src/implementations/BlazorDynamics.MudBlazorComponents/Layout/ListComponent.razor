@namespace BlazorDynamics.MudBlazorComponents.Layout
@inherits  BlazorDynamics.Forms.Components.Layout.ListComponentBase


<BDFormComponentWrapper Class="listcomponent" FormComponent="@this">
    @Label
    <MudStack>

        @for (int i = 0; i < _listValue.Count; i++)
        {
            @foreach (var element in this.FormModel.Elements)
            {
                <DynamicComponent Type=@GetComponentTypeFor(element) Parameters=@GetValidParametersFor(element, i) />
            }
        }
    </MudStack>
</BDFormComponentWrapper>


